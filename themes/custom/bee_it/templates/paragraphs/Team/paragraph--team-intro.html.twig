<header class="b-header b-header--team">
  <div class="l-container">
    <div class="b-header__hexagon-background b-header__hexagon-background--left">
      <img src="{{ file_url(elements.field_image['#items'].entity.uri.value ) }}" alt="" />
    </div>
    <div class="b-header__headline" intro-animate>
      <div class="l-container">
        <div class="">
          <h1 class="b-header__title h1">{{ content.field_title }}</h1>
          <div class="b-header__sub-title">{{ content.field_subtitle }}</div>
          <hr class="orange">
          <p class="b-header__body"> {{ content.field_description }}</p>
        </div>
      </div>
      <div>
        {{ content.field_team_image }}
      </div>
    </div>
  </div>
</header>